    "@types/express": "^4.0.38",
    "@types/fs-extra": "^4.0.4",
    "@types/uglify-js": "^2.6.29",
    "ajv": "^5.3.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.2.0",
