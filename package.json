{
  "name": "preboot",
  "version": "6.0.0-beta.4",
  "description": "Record server view events and play back to Angular client view",
  "scripts": {
    "postinstall": "ngc -p angular.tsconfig.json",
    "e2e": "cd integration && npm run e2e",
    "lint": "tslint -c tslint.json --project ./tsconfig.json -t verbose",
    "preskylint": "bazel build --noshow_progress @io_bazel//src/tools/skylark/java/com/google/devtools/skylark/skylint:Skylint",
    "skylint": "find . -type f -name \"*.bzl\" ! -path \"*/node_modules/*\" ! -path \"./dist/*\" | xargs $(bazel info bazel-bin)/external/io_bazel/src/tools/skylark/java/com/google/devtools/skylark/skylint/Skylint",
    "prebuildifier": "bazel build --noshow_progress @com_github_bazelbuild_buildtools//buildifier",
    "buildifier": "find . -type f \\( -name BUILD -or -name BUILD.bazel \\) ! -path \"*/node_modules/*\" | xargs $(bazel info bazel-bin)/external/com_github_bazelbuild_buildtools/buildifier/*/buildifier"
  },
  "author": "Jeff Whelpley",
  "license": "MIT",
  "contributors": [
    "Tobias Bosch <tbosch@google.com>",
    "PatrickJS <github@gdi2290.com>",
    "Jeff Whelpley <jeff@gethuman.com>"
  ],
  "devDependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/bazel": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/platform-server": "^6.1.0",
    "@bazel/ibazel": "^0.3.1",
    "@types/express": "^4.11.1",
    "@types/jasmine": "^2.8.7",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^8.10.14",
    "@types/uglify-js": "^2.6.31",
    "ajv": "^5.5.2",
    "camelcase": "^4.1.0",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "http-server": "^0.10.0",
    "jasmine": "^2.99.0",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "minimatch": "^3.0.4",
    "protractor": "^5.3.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.5",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^6.1.0",
    "systemjs": "0.19.43",
    "ts-loader": "^3.5.0",
    "ts-node": "^3.3.0",
    "tsickle": "^0.32.1",
    "tslib": "^1.8.0",
    "tslint": "^5.10.0",
    "tsutils": "^2.26.1",
    "typescript": "~2.9.2",
    "uglify-js": "^3.3.25",
    "webdriver-manager": "^12.0.6",
    "zone.js": "^0.8.26"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/preboot"
  },
  "bugs": {
    "url": "https://github.com/angular/preboot/issues"
  },
  "homepage": "https://github.com/angular/preboot"
}
